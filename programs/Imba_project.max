    {
        print("Введите пароль:");
        input = input();
        if (input == "228")
        {
            print("Введите x:");
            x = toint(input());
            print("Введите y:");
            y = toint(input());
        }
        if (input != "228")
        {
            println("пароль неверный");
        }
    }
}